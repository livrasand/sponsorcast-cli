{
  "name": "sponsorcast-cli",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool for uploading videos to Sponsorcast - exclusive video platform for GitHub Sponsors",
  "main": "./bin/sponsorcast.js",
  "keywords": [
    "sponsorcast",
    "github",
    "sponsors",
    "video",
    "hls",
    "cli",
    "upload",
    "streaming",
    "content-creation"
  ],
  "author": {
    "name": "livrasand",
    "url": "https://github.com/livrasand"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livrasand/sponsorcast-cli.git"
  },
  "bugs": {
    "url": "https://github.com/livrasand/sponsorcast-cli/issues"
  },
  "homepage": "https://sponsorcast.vercel.app",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "bin": {
    "sponsorcast": "./bin/sponsorcast.js"
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "start": "node ./bin/sponsorcast.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run validate",
    "validate": "node ./bin/sponsorcast.js --help",
    "upload:dev": "cross-env NODE_ENV=development node ./bin/sponsorcast.js upload",
    "upload:prod": "cross-env NODE_ENV=production node ./bin/sponsorcast.js upload"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^5.4.1",
    "commander": "^11.1.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.7",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "ffmpeg-static": "^5.2.0"
  }
}
